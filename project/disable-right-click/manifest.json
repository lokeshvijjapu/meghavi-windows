{
  "manifest_version": 3,
  "name": "Context Menu Blocker",
  "version": "1.1",
  "description": "Blocks right-click context menu on all pages",
  "permissions": [],
  "content_scripts": [
  {
    "matches": ["<all_urls>"],
    "js": ["blocker.js"],
    "all_frames": true,
    "run_at": "document_start"
  }
]

}
